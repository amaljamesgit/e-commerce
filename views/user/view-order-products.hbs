<section>
    <div class="container mt-5 mb-2">
        <div class="row">
            {{#each products}}


            <div class="col-md-3 p-3">
                <div class="card" style="width: 13rem;height: 25rem;">
                    <img src="/product-images/{{this.product._id}}.jpg" style="height: 12rem;" class="card-img-top" alt="...">
                    <div class="card-body">
                        <h3 class="card-title">{{this.product.name}}</h3>
                        <h5 class="card-text">{{this.category}}</h5>
                        <h6 class="card-text">Quantity{{this.product.quantity}}</h6>
                        <h5 class="card-title">RS.{{this.product.rate}}</h5>

                        
                    </div>
                </div>
            </div>
            {{/each}}


        </div>
    </div>
</section>
